{
  "name": "be-let",
  "version": "0.0.2",
  "description": "provides a light-weight way of turning a section of HTML into tiny unnamed islands of interactivity, modeled after be- based behavior -- be-lets",
  "main": "be-let.js",
  "module": "be-let.js",
  "exports": {
    ".": "./be-let.js",
    "./be-let.js": "./be-let.js"
  },
  "files": [
    "*.*"
  ],
  "types": "types.d.ts",
  "keywords": [
    "web-components",
    "custom-elements"
  ],
  "scripts": {
    "test": "playwright test",
    "package-check": "package-check",
    "serve": "node node_modules/may-it-serve/serve.js"
  },
  "dependencies": {
    "be-watching": "0.0.11",
    "be-hive": "0.0.70",
    "be-decorated": "0.0.165",
    "be-exportable": "0.0.37",
    "be-scoped": "0.0.1",
    "trans-render": "0.0.618"
  },
  "devDependencies": {
    "may-it-serve": "0.0.2",
    "@playwright/test": "1.26.1",
    "@skypack/package-check": "0.2.2",
    "@custom-elements-manifest/analyzer": "0.6.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/be-let.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/be-let/issues"
  },
  "homepage": "https://github.com/bahrus/be-let#readme"
